{
    "name": "ibnnajjaar/dhiraagu-sms-laravel",
    "description": "A simple, lightweight package for sending SMS using Dhiraagu SMS API",
    "keywords": [
        "ibnnajjaar",
        "dhiraagu",
        "sms",
        "laravel",
        "notification"
    ],
    "homepage": "https://github.com/ibnnajjaar/dhiraagu-sms-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Hussain Afeef",
            "email": "ibnahnajjaar@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "illuminate/support": "^12.00"
    },
    "require-dev": {
        "orchestra/testbench": "^10.6",
        "pestphp/pest": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "IbnNajjaar\\DhiraaguSMSLaravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IbnNajjaar\\DhiraaguSMSLaravel\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IbnNajjaar\\DhiraaguSMSLaravel\\DhiraaguSMSLaravelServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-clover=coverage.xml",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
